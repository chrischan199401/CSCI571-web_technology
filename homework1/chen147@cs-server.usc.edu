      adImages = new Array("pic.jpg", "pic2.jpg", "pic3.jpg");
      adURLs = new Array("WWW.baidu.com", "www.google.com", "www.taobao.com");
      thisAd = 0;
    }
    function cycleAds(){
      if(document.images){
        if(document.adBanner.complete){
          if(++thisAd == adImages.length)
            thisAd = 0;

          document.adBanner.src = adImages[thisAd];
        }
      }
      setTimeout("cycleAds()", 3000);

    }
    function gotoAd(){
      document.location.href = "http://"+ adURLs[thisAd];
    }

  </script>

